#include "calculos.h"

float resultWithDiscount(int number1, int defaultDiscount){
	float discount;
	float resultWithDiscount;
	discount = number1 * ((float)defaultDiscount / 100);
	resultWithDiscount = number1 - discount;
	return resultWithDiscount;
}
